<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>
	<appSettings>
		<add key="database" value="sqlite" />
	</appSettings>
	<connectionStrings>
		<add name="sqlite" connectionString="Data Source=../../../../db/db.sqlite;Version=3" providerName="System.Data.SQLite" />
		<add name="mysql" connectionString="server=localhost;database=nbooks;user id=root" providerName="MySql.Data.MySqlClient" />
	</connectionStrings>
	<system.data>
		<DbProviderFactories>
			<remove invariant="MySql.Data.MySqlClient" />
			<add name="MySQL Data Provider" invariant="MySql.Data.MySqlClient" description=".Net Framework Data Provider for MySQL" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.9.9.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
		</DbProviderFactories>
	</system.data>
</configuration>