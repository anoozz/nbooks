<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="NBooks.Core.Models" assembly="NBooks.Core">
	<class name="WorkersComp" table="workers_comps">
		<id name="Id" column="id">
			<generator class="identity" />
		</id>
		<property name="Code" column="code" />
		<property name="Description" column="description" />
		<property name="Rate" column="rate" />
		<property name="EffectivityDate" column="effectivity_date" />
		<property name="Inactive" column="inactive" />
	</class>
</hibernate-mapping>